    "PRECIO_CAJETILLA = 75.00     # Precio 2025 (MXN)\n",
    "CIGARROS_PAQUETE = 20\n",
    "\n",
    "# Variables Macroecon√≥micas\n",
    "ANIOS_PROYECCION = 20        \n",
    "TASA_RETORNO_ANUAL = 0.10    # 10% (Media hist√≥rica conservadora)\n",
    "INFLACION_TABACO = 0.05      # 5% Aumento anual de precio\n",
    "\n",
    "# C√°lculo del Gasto Base (A√±o 0)\n",
    "gasto_diario = (CIGARROS_DIARIOS / CIGARROS_PAQUETE) * PRECIO_CAJETILLA\n",
    "gasto_anual_inicial = gasto_diario * 365\n",
    "\n",
    "print(f\"üí∞ Gasto Diario Base: ${gasto_diario:.2f} MXN\")\n",
    "print(f\"üî• Gasto Anual Inicial: ${gasto_anual_inicial:,.2f} MXN\")"
